<% if(typeof errors !='undefined') {%> <% errors.forEach(function(errors){ %>
<div
  class="alert alert-dissmissible fade show alert-warning fade show"
  role="alert"
>
  <strong><%=errors.msg %> </strong>
  <button
    type="button"
    class="close"
    type="button"
    data-bs-dismiss="alert"
    aria-label="Close"
  >
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% }); %> <% } %> <% if(success_msg != ''){ %>
<div
  class="alert alert-dissmissible fade show alert-success fade show"
  role="alert"
>
  <strong><%=success_msg %> </strong>
  <button
    type="button"
    class="close"
    type="button"
    data-bs-dismiss="alert"
    aria-label="Close"
  >
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %> <% if(error_msg != ''){ %>
<div
  class="alert alert-dissmissible fade show alert-warning fade show"
  role="alert"
>
  <strong><%=error_msg %> </strong>
  <button
    type="button"
    class="close"
    type="button"
    data-bs-dismiss="alert"
    aria-label="Close"
  >
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %> <% if(error != ''){ %>
<div
  class="alert alert-dissmissible fade show alert-warning fade show"
  role="alert"
>
  <strong><%=error %> </strong>
  <button
    type="button"
    class="close"
    type="button"
    data-bs-dismiss="alert"
    aria-label="Close"
  >
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>
